{
  "name": "nebula.css",
  "version": "1.0.0",
  "description": "A work-in-progress CSS framework with the aim to quickly generate “building-block” classes for font sizes, colors, background colors and spacing",
  "scripts": {
    "start": "grunt",
    "build-sass": "node-sass scss/nebula.scss dist/nebula.css --output-style compact --source-map true",
    "build-sass-custom": "node-sass tests/custom.scss dist/nebula.css --output-style compact --source-map true",
    "test": "node tests/index.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcohamersma/nebula.css.git"
  },
  "bugs": {
    "url": "https://github.com/marcohamersma/nebula.css/issues"
  },
  "homepage": "https://github.com/marcohamersma/nebula.css#readme",
  "main": "index.js",
  "dependencies": {
    "css-color-names": "0.0.1",
    "decamelize": "^1.2.0",
    "lodash.reduce": "^4.6.0",
    "mz": "^2.4.0",
    "node-sass": "3.10.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "camelize": "^1.0.0",
    "eslint": "1.4.1",
    "faucet": "0.0.1",
    "grunt": "^0.4.5",
    "grunt-contrib-sass": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "handlebars": "^3.0.1",
    "slug": "^0.9.1",
    "tape": "^4.6.2",
    "to-markdown": "^1.0.0-rc.4"
  },
  "keywords": [
    "css",
    "sass",
    "framework"
  ],
  "author": "Marco Hamersma",
  "license": "MIT"
}
